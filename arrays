#include <stdio.h>
#include <stdlib.h>

int main() {
    int n;
    int *arreglo;

    printf("Enter the long of the string: ");
    scanf("%d", &n);
    
    arreglo = (int *)malloc(n * sizeof(int));
    
    printf("Enter %d integers please:\n", n);
    for (int i = 0; i < n; i++) {
        printf("num %d: ", i + 1);
        scanf("%d", arreglo + i);
    }

    printf("\nElements in reverse order:\n");
    for (int i = n - 1; i >= 0; i--) {
        printf("Element %d: %d\n", i + 1, *(arreglo + i));
    }
    
    free(arreglo);
    
    return 0;
}
